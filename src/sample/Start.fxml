<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane layoutY="64.0" prefHeight="416.0" prefWidth="800.0" stylesheets="@style.css"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.StartController">
   <Label layoutX="205.0" layoutY="35.0" text="Решение задачи о &quot;неограниченном рюкзаке&quot;.">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <Label layoutX="97.0" layoutY="75.0" text="Предмет может повторяться в рюкзаке неограниченное количество раз.">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <Label layoutX="57.0" layoutY="120.0"
          text="Для слишком большого числа предметов предусмотрен более быстрый алгоритм,">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <Label layoutX="213.0" layoutY="140.0" text="но он не гарантирует оптимальное решение.">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <Label layoutX="257.0" layoutY="220.0" text="Грузоподъемность рюкзака:">
      <font>
         <Font size="22.0"/>
      </font>
   </Label>
   <Slider fx:id="sliderSize" blockIncrement="1.0" layoutX="50.0" layoutY="270.0" majorTickUnit="1.0" min="1.0"
           minorTickCount="0" prefHeight="14.0" prefWidth="700.0" showTickLabels="true" showTickMarks="true"
           snapToTicks="true">
      <cursor>
         <Cursor fx:constant="HAND"/>
      </cursor>
   </Slider>
   <Hyperlink layoutX="595.0" layoutY="30.0" onMouseClicked="#wikiClick" style="-fx-text-fill: #3f51b5;" text="Wiki?"
              underline="true"/>
   <Label fx:id="labelSize" alignment="CENTER" contentDisplay="RIGHT" layoutX="370.0" layoutY="312.0" prefHeight="38.0"
          prefWidth="61.0" text=" " textAlignment="CENTER">
      <font>
         <Font size="20.0"/>
      </font>
   </Label>
   <Button layoutX="730.0" layoutY="350.0" maxHeight="50.0" maxWidth="50.0" mnemonicParsing="false"
           onMouseClicked="#toItems" prefHeight="50.0" prefWidth="50.0" text="➔" wrapText="true">
      <font>
         <Font size="33.0"/>
      </font>
   </Button>
</AnchorPane>
